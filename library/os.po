#: ../../library/os.rst:3727
msgid ""
"import os\n"
"\n"
"# semaphore with start value '1'\n"
"fd = os.eventfd(1, os.EFD_SEMAPHORE | os.EFC_CLOEXEC)\n"
"try:\n"
"    # acquire semaphore\n"
"    v = os.eventfd_read(fd)\n"
"    try:\n"
"        do_work()\n"
"    finally:\n"
"        # release semaphore\n"
